<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1%22%3E" fx:controller="ulb.info307.g6.views.CreateDeck">
    <Text layoutX="250.0" layoutY="100.0" text="Choose the name of your deck" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0" AnchorPane.topAnchor="100.0"  />
    <TextField fx:id="inputBox"  layoutX="200.0" layoutY="180.0" prefWidth="200.0" promptText="Enter text here" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0" AnchorPane.topAnchor="150.0" />
    <Button fx:id="buttonConfirm" mnemonicParsing="false" onAction="#clickConfirm" text="Confirm" AnchorPane.leftAnchor="150.0" AnchorPane.rightAnchor="150.0" AnchorPane.topAnchor="200.0" layoutX="250.0" layoutY="180.0" />
    <Button fx:id="buttonBack" mnemonicParsing="false" onAction="#clickBack" text="Back" AnchorPane.leftAnchor="150.0" AnchorPane.rightAnchor="150.0" AnchorPane.bottomAnchor="50.0" layoutX="250.0" layoutY="180.0" />
</AnchorPane>
