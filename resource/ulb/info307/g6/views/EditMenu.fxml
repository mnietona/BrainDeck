<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ulb.info307.g6.views.EditMenu" spacing="10">
   <HBox alignment="TOP_RIGHT" spacing="10">
      <Button fx:id="buttonEdit" mnemonicParsing="false" onAction="#clickEdit" text="Edit" />
      <Button fx:id="buttonAdd" mnemonicParsing="false" onAction="#clickAdd" text="Add" />
      <Button fx:id="buttonRemove" mnemonicParsing="false" onAction="#clickRemove" text="Remove" />
   </HBox>
   <Region VBox.vgrow="ALWAYS" />
   <HBox alignment="BOTTOM_RIGHT" spacing="10">
      <Button fx:id="buttonBack" mnemonicParsing="false" onAction="#backButtonAction" text="Back" />
   </HBox>
</VBox>
